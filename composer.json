{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=5.6.4",
    "laravel/framework": "5.4.*",
    "laravel/tinker": "~1.0",
    "rospdf/pdf-php": "0.12.*",
    "barryvdh/laravel-ide-helper": "v2.*",
    "doctrine/dbal": "^2.5",
    "spatie/laravel-permission": "^1.4",
    "barryvdh/laravel-debugbar": "^2.3",
    "smarty/smarty-lexer": "^3.1",
    "owen-it/laravel-auditing": "^4.0",
    "propaganistas/laravel-phone": "^2.8"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~5.7"
  },
  "autoload": {
    "classmap": [
      "database",
      "legacy/classes/"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "legacy/includes/allgemeine_funktionen.php",
      "legacy/includes/formular_funktionen.php",
      "legacy/includes/haus_form_funktionen.php"

    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "php artisan ide-helper:generate",
      "php artisan view:clear",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan ide-helper:generate",
      "php artisan view:clear",
      "php artisan optimize"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  }
}

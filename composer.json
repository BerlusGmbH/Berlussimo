{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.1.3",
    "beyondcode/laravel-websockets": "^1.1",
    "damienlagae/xlsxwriter": "^1.0",
    "doctrine/dbal": "^2.5",
    "fideloper/proxy": "^4.0",
    "kingsquare/php-mt940": "dev-berlussimo",
    "laravel/framework": "5.7.*",
    "laravel/homestead": "^8.1",
    "laravel/passport": "^7.0",
    "laravel/tinker": "^1.0",
    "mashape/unirest-php": "^3.0",
    "owen-it/laravel-auditing": "^8.0",
    "predis/predis": "^1.1",
    "propaganistas/laravel-phone": "^2.8",
    "rospdf/pdf-php": "0.12.40",
    "smarty/smarty-lexer": "3.1.29",
    "spatie/laravel-permission": "^1.4"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^v2.5",
    "beyondcode/laravel-dump-server": "^1.0",
    "filp/whoops": "^2.0",
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^2.0",
    "phpunit/phpunit": "^7.0",
    "symfony/thanks": "^1.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "classmap": [
      "database/seeds",
      "database/factories",
      "legacy/classes/"
    ],
    "files": [
      "config/phpsettings.php",
      "legacy/includes/allgemeine_funktionen.php",
      "legacy/includes/formular_funktionen.php",
      "legacy/includes/haus_form_funktionen.php"

    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/BerlusGmbH/php-mt940.git"
    }
  ],
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "post-update-cmd": [
      "@php artisan ide-helper:meta"
    ]
  }
}

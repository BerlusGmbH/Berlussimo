		<xs:complexContent>
			<xs:restriction base="PersonIdentification5">
				<xs:sequence>
					<xs:element name="Othr" type="AT_GenericPersonIdentification1" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:restriction>
		</xs:complexContent>
	</xs:complexType>
